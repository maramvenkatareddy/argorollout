--- # active service
kind: Service
apiVersion: v1
metadata:
  name: superapi-svc-active
spec:
  selector:
    app: superapi
    track: 1.0
  type: NodePort
  ports:
  - name: http
    port: 80
    targetPort: 80

--- # preview service
kind: Service
apiVersion: v1
metadata:
  name: superapi-svc-preview
spec:
  selector:
    app: superapi
    track: 2.0
  type: NodePort
  ports:
  - name: http
    port: 80
    targetPort: 80
